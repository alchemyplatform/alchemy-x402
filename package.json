{
  "name": "@alchemy/x402",
  "version": "0.1.0",
  "description": "CLI and library for Alchemy x402 authentication and payments",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "alchemy-x402": "./dist/cli/index.mjs"
  },
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "npm run build && changeset publish"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@x402/core": "^2.4.0",
    "@x402/evm": "^2.4.0",
    "@x402/fetch": "^2.4.0",
    "commander": "^14.0.3",
    "ms": "^2.1.3",
    "viem": "^2.46.3"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@types/ms": "^2.1.0",
    "@types/node": "^25.3.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
